{
	"manifest_version": 2,
	"name": "bookmark",
	"version": "1.0.0",
	"icons": {"16": "icon.png", "48": "icon.png", "128": "icon.png"},
	"background": {
		//"page": "bkg.html",
		"scripts": ["bkg.js"]
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "bookmark sync",
		"default_popup": "popup.html" 
	},
	"permissions": ["bookmarks"]
}